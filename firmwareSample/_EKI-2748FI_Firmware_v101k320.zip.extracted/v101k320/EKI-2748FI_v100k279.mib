SwitchDHCPServerCfgEntry ::= SEQUENCE
{
    swDHCPServerCfgIndex      Integer32,
    swDHCPServerCfgStatus     INTEGER,
    swDHCPServerCfgLowIPAddr  IpAddress,
    swDHCPServerCfgHighIPAddr IpAddress,
    swDHCPServerCfgSubMask    IpAddress,
    swDHCPServerCfgGateway    IpAddress,
    swDHCPServerCfgDNS        IpAddress,
    swDHCPServerCfgLeaseTime  Integer32
}

swDHCPServerCfgIndex OBJECT-TYPE
